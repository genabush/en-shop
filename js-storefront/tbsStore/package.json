{
  "name": "mystore-beta-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test-c": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e:clean": "node ./node_modules/protractor/bin/webdriver-manager clean && node ./node_modules/protractor/bin/webdriver-manager update --standalone false --gecko false --versions.chrome",
    "e2e": "ng e2e --webdriver-update false",
    "format:fix": "pretty-quick --staged",
    "test-headless": "ng test --browsers=HeadlessChrome --watch=false",
    "pre-push-commands": "ng test --browsers=HeadlessChrome --watch=false && ng lint && ng e2e --webdriver-update false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run-s format:fix",
      "pre-push": "npm run-s test-headless"
    }
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "^11.0.5",
    "@angular/forms": "~8.1.0",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/pwa": "^0.801.0",
    "@angular/router": "~8.1.0",
    "@angular/service-worker": "~8.1.0",
    "@ng-bootstrap/ng-bootstrap": "5.0.0-rc.1",
    "@ng-select/ng-select": "^2.20.5",
    "@ngrx/effects": "~8.5.2",
    "@ngrx/entity": "^8.5.2",
    "@ngrx/router-store": "~8.5.2",
    "@ngrx/store": "~8.5.2",
    "@ngrx/store-devtools": "^8.5.2",
    "@spartacus/assets": "~1.3.1",
    "@spartacus/core": "~1.3.1",
    "@spartacus/storefront": "~1.3.1",
    "@spartacus/styles": "~1.3.1",
    "bootstrap": "^4.2.1",
    "core-js": "^3.1.4",
    "i18next": "^17.0.6",
    "i18next-xhr-backend": "^3.0.0",
    "lory.js": "^2.5.3",
    "ng-mocks": "^8.1.0",
    "ngx-infinite-scroll": "^8.0.1",
    "ngx-paypal": "^5.0.0",
    "ngx-swiper-wrapper": "^8.0.2",
    "ngx-webstorage-service": "^4.1.0",
    "node-sass": "^4.13.0",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.22",
    "@angular/cli": "^8.0.2",
    "@angular/compiler-cli": "^8.0.2",
    "@angular/language-service": "~8.1.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.146",
    "@types/node": "~12.0.12",
    "codelyzer": "^5.0.1",
    "diff": "^4.0.1",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^6.2.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "puppeteer": "^2.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "^3.4.5"
  }
}
